{
  "name": "novabyte-blog",
  "version": "0.0.1",
  "description": "My blog website.",
  "productName": "novabyte.blog",
  "author": "novabyte <novabyte@novabyte.blog>",
  "private": true,
  "scripts": {
    "upgrade-quasar": "yarn dlx -p @quasar/cli quasar upgrade -i",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "clean": "rm -r dist",
    "dev-ssr": "quasar dev -m ssr",
    "dev-build-ssr": "quasar build -dm ssr",
    "build-ssr": "quasar build -m ssr",
    "start": "node dist/ssr/index.js",
    "docker-build": "docker build -t novabyte-ui:latest .",
    "docker-run": "docker run -p 52002:52002 novabyte-ui:latest"
  },
  "dependencies": {
    "@lucide/lab": "^0.1.2",
    "@quasar/extras": "^1.16.12",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.2.1",
    "d3-interpolate": "^3.0.1",
    "dayjs": "^1.11.11",
    "dompurify": "^3.1.2",
    "dotenv": "^16.4.5",
    "http-proxy-middleware": "^3.0.3",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.446.0",
    "marked": "^12.0.2",
    "picocolors": "^1.1.1",
    "pinia": "^2.0.11",
    "postcss": "^8.4.29",
    "quasar": "^2.18.1",
    "sanitize-html": "^2.13.0",
    "surrealdb.js": "^1.0.0-beta.9",
    "ulidx": "^2.2.1",
    "validator": "^13.11.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "2.0.0-beta.24",
    "@quasar/vite-plugin": "1.8.1",
    "@types/axios": "^0.14.0",
    "@types/dompurify": "^3.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/marked": "^5.0.1",
    "@types/node": "^18.0.0",
    "@types/sanitize-html": "^2.13.0",
    "@types/validator": "^13.11.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "http-proxy-middleware": "^3.0.3",
    "prettier": "^2.5.1",
    "sass-embedded": "^1.80.2",
    "typescript": "^5.4.0",
    "vite": "5.2.10"
  },
  "engines": {
    "node": "^20 || ^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "packageManager": "yarn@4.9.1"
